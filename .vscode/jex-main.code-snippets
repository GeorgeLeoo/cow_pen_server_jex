	// Example:
	"jex-main": {
		"scope": "javascript,typescript",
		"prefix": "jex-main",
		"body": [
			"module.exports = {",
			"\tprefix: '$1',",
			"\turl: '$2',",
			"\tmethod: '$3',",
			"\trequestParams: [",
			"\t\t//{ value: 'username', errorMsg: '用户名不能为空', rule: 'empty' },",
